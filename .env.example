# Copy this file to .env and configure your values

# === PROJECT CONFIGURATION ===
PROJECT_NAME=postgresql-migration-system
PROJECT_VERSION=1.0.0
ENVIRONMENT=development

# === LOGGING CONFIGURATION ===
MIGRATION_LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=logs/migration.log

# === DATABASE CONFIGURATION ===
# Source Database
SOURCE_HOST=your-source-host
SOURCE_PORT=5432
SOURCE_DATABASE=your_source_db
SOURCE_USER=migration_user
SOURCE_PASSWORD=your_secure_password

# Destination Database
DESTINATION_HOST=your-destination-host
DESTINATION_PORT=5432
DESTINATION_DATABASE=your_destination_db
DESTINATION_USER=migration_user
DESTINATION_PASSWORD=your_secure_password

# === MIGRATION CONFIGURATION ===
MIGRATION_STRATEGY=hybrid
BATCH_SIZE=10000
PARALLEL_WORKERS=8
CHUNK_SIZE_MB=100
TIMEOUT_SECONDS=3600
RETRY_ATTEMPTS=3
VALIDATION_LEVEL=full

# === MONITORING CONFIGURATION ===
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
ENABLE_METRICS=true
PROGRESS_REPORTING_INTERVAL=30
HEALTH_CHECK_INTERVAL=60

# === SECURITY CONFIGURATION ===
ENCRYPTION_IN_TRANSIT=true
SSL_VERIFICATION=true
CREDENTIAL_MANAGEMENT=secrets
AUDIT_LOGGING=true

# === BACKUP CONFIGURATION ===
ENABLE_PRE_MIGRATION_BACKUP=true
BACKUP_RETENTION_DAYS=30
BACKUP_COMPRESSION=true
ROLLBACK_STRATEGY=automatic

# === PERFORMANCE CONFIGURATION ===
CONNECTION_POOLING=true
ASYNC_OPERATIONS=true
BULK_OPERATIONS=true
SHARED_BUFFERS_MB=4096
WORK_MEM_MB=256
MAINTENANCE_WORK_MEM_MB=1024

# === DEVELOPMENT CONFIGURATION ===
DEBUG_MODE=false
ENABLE_TEST_MODE=false
ENABLE_DRY_RUN=false
MOCK_EXTERNAL_SERVICES=false

# === PATHS ===
SECRETS_PATH=./secrets
CONFIG_PATH=./config
LOG_PATH=./logs
REPORTS_PATH=./reports
BACKUP_PATH=./backups

# === PYTHON CONFIGURATION ===
PYTHONPATH=.
PYTHONUNBUFFERED=1
